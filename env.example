# Convex Configuration
# Deployment name: scintillating-gerbil-351
# Copy file này thành .env.local và thay thế CONVEX_DEPLOY_KEY

# URL của Convex deployment
NEXT_PUBLIC_CONVEX_URL=https://scintillating-gerbil-351.convex.cloud

# Deploy key cho production (lấy từ convex dashboard hoặc output của 'convex dev')
CONVEX_DEPLOY_KEY=your_deploy_key_here

# Cloudinary Configuration (for image uploads on Vercel)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret